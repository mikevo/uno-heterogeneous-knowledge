<div class="quiz-edit">
  <div class="container">
    <%= form_for(@quiz) do |f| %>
      <div class="form-group">
        <label for="title">New Quiz Title</label>
        <%= f.text_field :title, class: "form-control" %>
      </div>
      <%= f.submit "Update", :class => "btn btn-primary" %>
    <% end %>
  </div>
</div>
